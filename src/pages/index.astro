---
import { buttonVariants } from "@/components/ui/button";
import { siteConfig } from "@/config/site";
import MainLayout from "@/layouts/main-layout.astro";
import { cn } from "@/lib/utils";
import Icon from "astro-icon";
import IconsAnim from "../components/IconsAnim.astro";

import BlogHeader2 from "@/components/blog-header2.astro";
import BlogCard from "@/components/cards/blog-card2.astro";
import BlogCard2 from "@/components/cards/showcase-card.astro";
import { getPosts, getShowcase } from "@/lib/fetchers";

const posts = await getPosts();
const postsServizi = await getShowcase();

---

<MainLayout title="Livedata">
  <section id="iconBlock1" class="space-y-6 pb-12 pt-16 lg:py-28">
    <div
      class="container flex max-w-[64rem] flex-col items-center gap-5 text-center"
    >
      <a
        href="https://twitter.com/livedata"
        class={cn(buttonVariants({ variant: "outline", size: "sm" }))}
        target="_blank"
      >
        Seguici su <Icon name="twitter" class="ml-2 size-4" />
      </a>

      <h1
        class="font-heading text-4xl font-bold sm:text-5xl md:text-6xl lg:text-7xl !leading-[1.1] text-balance"
      >
      <span class="text-gradient_indigo-purple"><span class="font-sans font-bold">Velocità,</span></span> chiarezza, impatto

      </h1>

      <p class="max-w-[42rem] leading-normal text-muted-foreground sm:text-xl sm:leading-8 text-balance"
      >
      Siti web moderni, esperienza fluida. Contenuti strategici.
      </p>

      <div class="flex justify-center space-x-2 md:space-x-4">
        <a
          href="/login"
          class={cn(buttonVariants({ size: "lg" }))}
        >
        Richiedi Preventivo
        </a>
        <a
          href=/servizi
          target="_self"
          rel="noreferrer"
          class={cn(buttonVariants({ variant: "outline", size: "lg" }), "px-4")}
        >
          <!-- <Icon name="two-lines" class="mr-2 size-5" /> -->
           Servizi Web
        </a>
      </div>
    </div>

  
    
   
  </section>

  



  <section
    id="features"
    class="container space-y-6 bg-slate-50 py-8 dark:bg-transparent md:py-12 lg:py-24"
  >
    <div
      class="mx-auto flex max-w-[58rem] flex-col items-center space-y-4 text-center"
    >
      <h2 class="font-heading text-3xl leading-[1.1] sm:text-3xl md:text-6xl">
        Redefinendo l’esperienza digitale
      </h2>
      <p
        class="max-w-[85%] leading-normal text-muted-foreground sm:text-lg sm:leading-7 balance-text"
      >
      Sviluppo Web e Content Strategy guidati da tecniche moderne e interattive, collaborazione Umano-IA.
      </p>
    </div>
    <div
      class="mx-auto grid justify-center gap-4 sm:grid-cols-2 md:max-w-[64rem] md:grid-cols-3"
